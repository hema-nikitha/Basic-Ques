import java.util.*;
class Numofdig{
public staic void main(String[] args){
    Scanner sc=new Scanner(System.in);
    System.out.println("Enter");
    int num=sc.nextInt();
    int c=0;
    if(num>0){
        c=1;
    }
    int r=num%10;
    c++;
    System.out.println(c);


}



}